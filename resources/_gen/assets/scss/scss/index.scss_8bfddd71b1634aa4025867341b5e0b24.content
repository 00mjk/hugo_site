/*
Note from the relevant GitHub SCSS file:
"SFMono-Regular needs to come before SF Mono to fix an older version of the font in Chrome"
*/
.tweet-card {
  line-height: 1.35em;
  font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
  font-size: 1rem;
  margin: 1.5rem auto;
  padding: 1rem;
  border: 2px solid hsl(0deg, 0%, 35%);
  border-radius: 12px;
  background: hsl(0deg, 0%, 100%);
  width: 100%;
}
@media screen and (min-width: 768px) {
  .tweet-card {
    font-size: 1.25rem;
  }
}
.tweet-card p {
  margin-top: 0;
  margin-bottom: 1em;
  line-height: 1.35em;
}
@media screen and (min-width: 960px) {
  .tweet-card {
    width: 80%;
  }
}
@media screen and (min-width: 1024px) {
  .tweet-card {
    width: 85%;
  }
}
@media screen and (min-width: 1920px) {
  .tweet-card {
    max-width: 80ch;
  }
}
@media screen and (prefers-color-scheme: dark) {
  .tweet-card {
    background: hsl(210deg, 50%, 10%);
  }
}
.tweet-card a, .tweet-card a:hover {
  border: none !important;
  overflow-wrap: break-word;
  word-wrap: break-word;
  word-break: break-word;
  /*
  See:
  - https://css-tricks.com/snippets/css/prevent-long-urls-from-breaking-out-of-container/
  - https://css-tricks.com/where-lines-break-is-complicated-heres-all-the-related-css-and-html/
  - https://css-tricks.com/better-line-breaks-for-long-urls/
  */
}
@media screen and (prefers-color-scheme: dark) {
  .tweet-card a, .tweet-card a:hover {
    color: hsl(204deg, 100%, 83%);
  }
}
@media screen and (prefers-color-scheme: dark) {
  .tweet-card {
    border: 2px solid rgb(199, 199, 199);
    color: hsl(0deg, 0%, 100%);
  }
}

.tweet-body {
  text-align: left !important;
}

.tweet-images img {
  border-radius: 12px;
}

.tweet-media-img {
  width: 100%;
  height: auto;
  aspect-ratio: 1;
  object-fit: cover;
}

.tweet-card-img {
  border-bottom: 1px solid rgb(199, 199, 199);
  width: 100%;
}
@media screen and (prefers-color-scheme: dark) {
  .tweet-card-img {
    border: 1px solid hsl(0deg, 0%, 66%);
  }
}

.tweet-player {
  display: flex;
}

a > .card {
  color: hsl(0deg, 0%, 35%);
}
@media screen and (prefers-color-scheme: dark) {
  a > .card {
    color: hsl(0deg, 0%, 66%);
  }
}

.card {
  overflow: hidden;
  margin-top: 1em;
  border-radius: 12px;
  border: 1px solid rgb(199, 199, 199);
}
@media screen and (prefers-color-scheme: dark) {
  .card {
    border: 1px solid hsl(0deg, 0%, 66%);
  }
}
.card p {
  padding: 0.5em 1em 1em;
  line-height: 1.4;
  margin-top: 0;
  font-size: 0.85rem;
}
.card p .card-title {
  color: hsl(0deg, 0%, 0%);
}
@media screen and (prefers-color-scheme: dark) {
  .card p .card-title {
    color: hsl(0deg, 0%, 100%);
  }
}

.tweet-header {
  display: flex;
}
.tweet-header .tweet-profile {
  margin-right: 0.5em;
  margin-bottom: 1em;
  flex-shrink: 0;
}
.tweet-header .tweet-profile img {
  border-radius: 9999px;
  width: 48px;
  height: auto;
}
.tweet-header .tweet-author {
  display: flex;
  flex-direction: column;
  flex-grow: 1;
}
.tweet-header .tweet-author-name {
  font-weight: bold;
  font-size: 1rem;
  color: hsl(0deg, 0%, 0%);
  line-height: 1.375;
}
@media screen and (prefers-color-scheme: dark) {
  .tweet-header .tweet-author-name {
    color: hsl(0deg, 0%, 100%);
  }
}
.tweet-header .tweet-author-handle {
  color: hsl(0deg, 0%, 35%);
  font-size: 0.85rem;
  line-height: 1;
}
@media screen and (prefers-color-scheme: dark) {
  .tweet-header .tweet-author-handle {
    color: hsl(0deg, 0%, 66%);
  }
}
.tweet-header .tweet-author .tweet-bird {
  margin-left: 1.25rem;
}

.tweet-footer {
  margin-top: 1rem;
  display: flex;
  align-items: center;
  color: hsl(0deg, 0%, 35%);
  font-size: 0.85rem;
  /* === needed in Eleventy but not in Hugo
  .tweet-date:after {
  	content: " (UTC)";
  	font-size: var.$legal;
  }
  */
}
@media screen and (prefers-color-scheme: dark) {
  .tweet-footer {
    color: hsl(0deg, 0%, 66%);
  }
}
.tweet-footer .tweet-like-icon, .tweet-footer .tweet-like-count {
  display: none;
}
.tweet-footer a {
  color: hsl(0deg, 0%, 35%);
}
@media screen and (prefers-color-scheme: dark) {
  .tweet-footer a {
    color: hsl(0deg, 0%, 66%);
  }
}

[class^=tweet-img-grid],
.tweet-video-wrapper,
.tweet-media-video {
  margin-top: 0.5em;
  border-radius: 1em;
  overflow: hidden;
  display: grid;
  grid-template-columns: repeat(1, 1fr);
  grid-gap: 2px;
}

.tweet-video-wrapper {
  border-radius: 1em;
  display: block;
  position: relative;
}

.tweet-img-grid-3 {
  grid-template-columns: repeat(2, 1fr);
}

.tweet-img-grid-3 .tweet-media-img {
  aspect-ratio: 1.8;
}

.tweet-img-grid-3 :nth-child(1) {
  grid-row: 1/span 2;
  height: 100%;
}

.tweet-img-grid-3 :nth-child(1) .tweet-media-img {
  aspect-ratio: 0.9;
}

.tweet-img-grid-2,
.tweet-img-grid-4 {
  grid-template-columns: repeat(2, 1fr);
}

.tweet-sens-blur {
  filter: blur(5rem);
  position: relative;
}

.tweet-sens-blur-no {
  filter: none !important;
  z-index: 9999 !important;
  position: relative;
}

[class^=tweet-img-grid] {
  position: relative;
}

.blur-text {
  position: absolute;
  font-weight: bold;
  text-shadow: 0.125em 0.125em 0.1em hsl(0deg, 0%, 0%);
  width: 100%;
  top: 40%;
  color: white;
  text-align: center;
  font-size: 1.5em;
  line-height: 1.2;
}

.blur-text-no {
  display: none !important;
  background-color: black;
}

.tweet-poll-wrapper {
  display: grid;
  grid-template-columns: 3.5em 0.25fr 1fr;
  grid-gap: 1em;
  line-height: 1;
}
.tweet-poll-count {
  grid-column: 1;
  text-align: right;
}
.tweet-poll-meter {
  grid-column: 2;
}
.tweet-poll-meter meter {
  width: 100%;
}
.tweet-poll-title {
  grid-column: 3;
}

.tweet-quoted-tweet {
  border: 1px solid hsl(0deg, 0%, 35%);
  border-radius: 12px;
  padding: 1em 1em 0.5em 1em;
  margin-top: 0.75em;
  margin-bottom: 0;
}
@media screen and (prefers-color-scheme: dark) {
  .tweet-quoted-tweet {
    border: 1px solid hsl(0deg, 0%, 66%);
  }
}
.tweet-quoted-tweet p {
  font-size: 0.85rem;
  margin: 0 0 0.5em;
  line-height: 1.4;
}
.tweet-quoted-tweet p .tweet-quoted-tweet-profile-image {
  border-radius: 9999px;
  width: 18px;
  height: auto;
  display: inline;
}
.tweet-quoted-tweet p.tweet-quoted-tweet-head {
  margin-bottom: 0.25em;
}
.tweet-quoted-tweet p.tweet-quoted-tweet-head a {
  color: hsl(0deg, 0%, 35%);
}
@media screen and (prefers-color-scheme: dark) {
  .tweet-quoted-tweet p.tweet-quoted-tweet-head a {
    color: hsl(0deg, 0%, 66%);
  }
}
.tweet-quoted-tweet p.tweet-quoted-tweet-head a:hover {
  color: hsl(240deg, 100%, 33%);
}
@media screen and (prefers-color-scheme: dark) {
  .tweet-quoted-tweet p.tweet-quoted-tweet-head a:hover {
    color: hsl(204deg, 100%, 83%);
  }
}

.tweet-reply-to {
  margin-top: 0;
  padding-top: 0;
  line-height: 1.3em;
  color: hsl(0deg, 0%, 35%);
  font-size: 0.85rem;
}
@media screen and (prefers-color-scheme: dark) {
  .tweet-reply-to {
    color: hsl(0deg, 0%, 66%);
  }
}
.tweet-reply-to .tweet-like-icon, .tweet-reply-to .tweet-like-count {
  display: none;
}

