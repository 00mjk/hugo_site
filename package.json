{
  "name": "hugo_site",
  "description": "This is the Hugo version of the repo for https://brycewray.com.",
  "main": "postcss.config.js",
  "private": true,
  "scripts": {
    "clean": "rm -rf public",
    "start": "NODE_ENV=production npm-run-all clean dev:hugo",
    "startIP": "NODE_ENV=production npm-run-all clean devIP:hugo",
    "build": "NODE_ENV=production npm-run-all clean prod:hugo",
    "testbuild": "NODE_ENV=production npm-run-all clean testbuild:hugo",
    "testbuildIP": "NODE_ENV=production npm-run-all clean testbuildIP:hugo",
    "dev:hugo": "hugo server",
    "devIP:hugo": "hugo server --bind '192.168.254.10'",
    "prod:hugo": "hugo --gc --minify",
    "testbuild:hugo": "hugo server --gc --minify",
    "testbuildIP:hugo": "hugo server --gc --minify --bind '192.168.254.10'"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "browserslist": [
    "defaults",
    "maintained node versions"
  ],
  "dependencies": {},
  "engines": {
    "node": "12.x"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.4",
    "postcss-clean": "^1.2.0",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.0",
    "postcss-preset-env": "^6.7.0",
    "tailwindcss": "^2.0.2"
  }
}
